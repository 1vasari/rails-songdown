<%= form_for @setlist do |f| %>
  <p>
    <%= f.label :title %>
    <br />
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :date %>
    <br />
    <%= f.date_field :date %>
  </p>

  <p>
    <%= f.label :notes %>
    <br />
    <%= f.text_area :notes %>
  </p>

  <%= f.submit %>
<% end %>

<% if @setlist.errors.any? %>
  <h1>Error</h1>

  <ul>
    <% for error_message in @setlist.errors.full_messages %>
      <li>
        <%= error_message %>
      </li>
    <% end %>
  </ul>
<% end %>
